			<gml:usesVerticalCS xlink:href="#WaterSurfaceAtTimeInstantCS"/>
			<gml:verticalDatum xlink:href="#WaterSurfaceAtTimeInstantDatum"/>
		</gml:VerticalCRS>
	</gml:dictionaryEntry>
