<?xml version="1.0" encoding="UTF-8"?>
<om:CompositeObservation 
xmlns:xlink="http://www.w3.org/1999/xlink" 
xmlns:gml="http://www.opengis.net/gml/3.2" 
xmlns:om="http://www.opengis.net/om/1.0" 
